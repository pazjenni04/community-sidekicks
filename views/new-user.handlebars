 <div class="col-md-6">
    <h2>Don't have an account? Create a new account.</h2>

    <form class="form signup-form">
      <div class="form-group">
        <label for="name-signup">First Name</label>
        <input class="form-input" type="text" id="firstName" />
      </div>
      <div class="form-group">
        <label for="name-signup">Last Name</label>
        <input class="form-input" type="text" id="lastName" />
      </div>
      <div class="form-group">
        <label for="email-signup">Email</label>
        <input class="form-input" type="text" id="email-signup" />
      </div>
      <div class="form-group">
        <label for="password-signup">Password</label>
        <input class="form-input" type="password" id="password-signup" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit" id="signupBtn">Signup</button>
      </div>
    </form>
  </div>
</div>

<script src="./js/login.js"></script>