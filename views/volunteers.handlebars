<!-- Shows all the users volunteer history/ log a new project!--> 
 <section class="UserContainer">
  {{#each allVolunteers as |volunteer| }}
 
    <h3> Full name: {{volunteer.first_name}} {{volunteer.last_name}}</h3>
      <p>Email: {{volunteer.email}}</p>
      <p>Phone number: {{volunteer.phone}}</p>
      <p>Phone number: {{volunteer.zip_code}}</p>
  </div>
  {{/each}}
</section>